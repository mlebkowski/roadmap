{% extends 'layouts/roadmap.layout.twig' %}
{% set activeMenu = 'team' %}
{% block title %}{{ 'title.team' | trans({ "%accountName%": account.name }) }}{% endblock %}

{% block content %}
    <div class="col-md-6 col-md-offset-3" id="team-page">
        <ul class="list-unstyled">
        {% for user in users %}
            <li class="row team-member">
                <img src="{{ user.gravatar }}" class="pull-right img-circle"/>
                <h3><a href="/user/{{ user.login }}">{{ user.name }}</a></h3>
                <p class="text-muted">{{ user.login }}</p>
            </li>

        {% endfor %}
        </ul>
    </div>
{% endblock %}